                        <script>
                                                        var arrowInitFunction = function() {
                                var arrows = document.querySelectorAll('.mpp');

                                if(arrows.length > 0) {
                                    var horizontalSmallDeviceHeight = 360;
                                    var deviceHeightHalf = window.innerHeight / 2;
                                    var topPadding = window.innerHeight <= horizontalSmallDeviceHeight ? deviceHeightHalf - arrows[0].clientHeight : 150;
                                    var docObject = document.querySelectorAll('.pdf, .page-doc')[0];
                                    var rect = docObject.getBoundingClientRect();
                                    var pxString = function pxString(string) {
                                        return string + "px";
                                    };
                                    var setButtonsPosition = function setButtonsPosition(position, x, y) {
                                        if (y === void 0) {
                                            y = 0;
                                        }

                                        for(var i = 0; i < arrows.length; ++i) {
                                            arrows[i].style.top = pxString(x)
                                            arrows[i].style.position = position
                                        }

                                        var prev = document.querySelectorAll('.mpp_prev');

                                        if(prev.length > 0) {
                                            prev[0].style.left = pxString(y);
                                        }

                                        var next = document.querySelectorAll('.mpp_next');

                                        if(next.length > 0) {
                                            next[0].style.right = pxString(y);
                                        }
                                    };
                                    var POS_ABSOLUTE = 'absolute';
                                    var padding;
                                    if (window.innerHeight <= horizontalSmallDeviceHeight) {
                                        padding = topPadding;
                                    } else {
                                        padding = rect.height * .2;
                                        padding = padding > topPadding ? topPadding : padding;
                                    }
                                    if (rect.top < topPadding) {
                                        rect.bottom - topPadding - padding * 2 <= 0 ?
                                            setButtonsPosition(POS_ABSOLUTE, rect.height - padding + docObject.offsetTop) :
                                            setButtonsPosition('fixed', topPadding + padding, rect.left);
                                    } else setButtonsPosition(POS_ABSOLUTE, padding + docObject.offsetTop);
                                }
                            }
                        </script>
